{
  
  "compilerOptions": {
    "target": "ES6" ,
    "module": "commonjs",
    "lib": ["DOM", "ES6", "ESNext.AsyncIterable"],
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node",
    "esModuleInterop":true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false

  }
}





